<application>
  <component name="DefaultFont">
    <option name="FONT_SIZE" value="16" />
    <option name="FONT_FAMILY" value="Hack" />
  </component>
  <component name="EditorSettings">
    <option name="languageBreadcrumbsMap">
      <map>
        <entry key="ECMAScript 6" value="true" />
        <entry key="HTML" value="true" />
        <entry key="JSX Harmony" value="true" />
        <entry key="JavaScript 1.5" value="true" />
        <entry key="PHP" value="true" />
      </map>
    </option>
    <option name="USE_SOFT_WRAPS" value="CONSOLE" />
    <option name="STRIP_TRAILING_SPACES" value="Whole" />
    <option name="IS_ENSURE_NEWLINE_AT_EOF" value="true" />
    <option name="SHOW_QUICK_DOC_ON_MOUSE_OVER_ELEMENT" value="true" />
    <option name="IS_RIGHT_MARGIN_SHOWN" value="false" />
    <option name="ARE_LINE_NUMBERS_SHOWN" value="false" />
    <option name="ARE_GUTTER_ICONS_SHOWN" value="false" />
    <option name="IS_FOLDING_OUTLINE_SHOWN" value="false" />
    <option name="SHOW_BREADCRUMBS_ABOVE" value="true" />
    <option name="SHOW_BREADCRUMBS" value="false" />
    <option name="IS_INDENT_GUIDES_SHOWN" value="false" />
    <option name="ADDITIONAL_PAGE_AT_BOTTOM" value="true" />
    <option name="IS_WHEEL_FONTCHANGE_ENABLED" value="true" />
  </component>
  <component name="LiveEditOptions" chromeUpdateApp="true" chromeUpdateOnChanges="true" nodeUpdateOnChanges="true" restartIfHotSwapFailed="true">
    <option name="elementHighlighting" value="true" />
  </component>
  <component name="TodoConfiguration">
    <pattern pattern="\btodo\b.*" />
    <pattern pattern="\bfixme\b.*" />
  </component>
  <component name="XmlEditorOptions">
    <option name="showCssInlineColorPreview" value="true" />
  </component>
</application>